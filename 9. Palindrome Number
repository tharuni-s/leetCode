class Solution {
    public boolean isPalindrome(int x) {
        int n=x,sum=0,rem=0;
        Boolean flag=false;
        while(x>0){
            rem=x%10;
            sum=sum*10+rem;
            x=x/10;
            }
        if(sum==n){
            flag=true;
        }
        return flag;
    }
}
